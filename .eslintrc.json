{
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/prop-types": "off",
    "arrow-parens": [2, "always"],
    "no-console": "off",
    "jsx-a11y/label-has-for": "off",
    "camelcase": [
      0,
      {
        "ignoreDestructuring": true
      }
    ]
  }
}
